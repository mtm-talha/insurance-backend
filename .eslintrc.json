{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["spellcheck"],
    "rules": {
      "no-console": 1,
      "no-var": 2,
      "camelcase": "off",
      "spellcheck/spell-checker": [
        1,
        {
          "comments": true,
          "strings": true,
          "identifiers": true,
          "templates": true,
          "lang": "en_US",
          "skipWords": [
          ],
          "skipIfMatch": ["http://[^s]*", "^[-\\w]+/[-\\w\\.]+$"],
          "skipWordIfMatch": ["^foobar.*$"],
          "minLength": 3
        }
      ]
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    }
  }